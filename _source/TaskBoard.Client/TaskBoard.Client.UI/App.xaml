<Application x:Class="TaskBoard.Client.UI.App"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:locators="clr-namespace:TaskBoard.Client.UI.Locators"
			 xmlns:viewModels="clr-namespace:TaskBoard.Client.UI.ViewModels"
			 StartupUri="Views/MainView.xaml">
	<Application.Resources>
		<ResourceDictionary>
			<locators:ViewModelLocator x:Key="Locator" />
			<viewModels:BoardViewModel x:Key="BoardViewModel" />
			<viewModels:ColumnViewModel x:Key="ColumnViewModel" />
			<viewModels:TaskViewModel x:Key="TaskViewModel" />
			<viewModels:UserViewModel x:Key="UserViewModel" />
			<viewModels:CommentViewModel x:Key="CommentViewModel" />
		</ResourceDictionary>
	</Application.Resources>
</Application>